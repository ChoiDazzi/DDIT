package kr.or.ddit.basic.args;

public class ArgsTest {
	public static void main(String[] args) {
		ArgsTest at = new ArgsTest();
		
		at.test01(0, "신짱구");
		System.out.println("=================");
		at.test01(3, "신짱아");
	}
	
	public void test01(int a, String str) {
		System.out.println("a= " + a);
		System.out.println("str= " + str);
	}
}
