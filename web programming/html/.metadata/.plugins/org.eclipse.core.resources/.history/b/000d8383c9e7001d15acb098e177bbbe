<title>script3</title>
<script>
	function proc1() {
		let today = new Date();
		
		let day1 = today.toLocaleString();
		
		//출력내용
		let str = "오늘은 " + today + "<br>";
		  	 str += "우리는 " + day1 + "<br>";
		  	 
		//id 가 res 인 요소를 검색 출력
		document.querySelecor('#res').innerHTML = today;
	}
</script>
</head>
<body>
	<input type="button" value="확인" onclick="proc1()">
	<div id="res"></div>
</body>
